# Auto detect text files and perform LF normalization
* text=auto

# Flutter specific
*.dart text
pubspec.yaml text
pubspec.lock text
analysis_options.yaml text

# Documentation
*.md text
*.txt text

# Images
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.svg text

# Archives
*.zip binary
*.tar.gz binary
*.jar binary

# IDE
*.iml text
.vscode/* text
.idea/* text

# Linguist language statistics
*.g.dart linguist-generated=true
*.freezed.dart linguist-generated=true
